  "devDependencies": {
    "chromedriver": "^2.46.0",
    "depcheck": "^0.7.2",
    "husky": "^1.3.1",
    "istanbul": "^0.4.5",
    "licensee": "^6.2.0",
    "lint-staged": "^8.1.5",
    "npm-authors-contributors": "^1.1.0",
    "rimraf": "^2.6.3",
    "standard": "^12.0.1",
    "tape": "^4.10.1",
    "webdriverio": "^4.13.2"
  },
  "scripts": {
    "build:humans": "./humans.txt.js > static/humans.txt",
    "coverage": "NODE_ENV=test istanbul cover -- tape test/*.test.js",
    "depcheck": "depcheck --ignores husky,chromedriver,lint-staged,standard,licensee",
    "postcoverage": "istanbul check --statements 100 --functions 100 --branches 100 --lines 100",
    "test": "NODE_ENV=test tape test/*.test.js",
    "testserver": "TMP=$(mktemp) ; ./make-keypair > $TMP ; . $TMP ; NODE_ENV=test PORT=8080 COMMISSION=1 DIRECTORY=example-directory node server.js ; rm $TMP",
    "lint": "standard",
    "licenses": "licensee --errors-only"
  },
  "lint-staged": {
    "linters": {
      "*.js": [
        "standard --fix",
        "git add"
      ]
    },
    "ignore": [
      "*.min.js"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged ; npm run build:humans"
    }
  }
}
